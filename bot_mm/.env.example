# === EXCHANGE CREDENTIALS ===
HL_PRIVATE_KEY=0x...
HL_WALLET_ADDRESS=0x...
HL_MODE=testnet

# === NOTIFICATIONS ===
DISCORD_WEBHOOK_URL=

# === SYMBOLS ===
# Enable multiple symbols: MM_SYMBOLS=BTCUSDT,ETHUSDT,SOLUSDT
MM_SYMBOLS=BTCUSDT

# === BTC MM PARAMETERS ===
# Typical HL spread ~1-2 bps, tight book
BTCUSDT_ENABLED=true
BTCUSDT_CAPITAL_USD=1000
BTCUSDT_SPREAD_BPS=2.0
BTCUSDT_VOL_MULT=1.5
BTCUSDT_SKEW_FACTOR=0.5
BTCUSDT_ORDER_SIZE_USD=100
BTCUSDT_NUM_LEVELS=1
BTCUSDT_LEVEL_SPACING_BPS=1.0
BTCUSDT_MAX_POS_USD=500
BTCUSDT_MAX_DAILY_LOSS=50
BTCUSDT_MAKER_FEE=-0.00015
BTCUSDT_TAKER_FEE=0.00045
BTCUSDT_EXCHANGE=hyperliquid

